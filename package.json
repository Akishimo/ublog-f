{
  "name": "ublog-f",
  "version": "1.2.0",
  "description": "frontend of ublog",
  "repository": {
    "type": "git",
    "url": "https://github.com/Akishimo/ublog-f.git"
  },
  "main": "webpack.config.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config build/dev.config.js",
    "build": "npm run static && npm run buildpage",
    "buildpage": "cross-env NODE_ENV=production webpack --config build/build.config.js --progress --hide-modules",
    "mocker": "nodemon mocker/server.js",
    "dm": "npm run mocker & npm run dev",
    "lintjs": "eslint --ext .js --ext .vue --format 'node_modules/eslint-friendly-formatter' ./",
    "lintcss": "stylelint **/*.less **/*.css **/*.vue",
    "lint": "npm run lintjs & npm run lintcss",
    "killmocker": "./kill.sh 4040",
    "static": "gulp --gulpfile ./src/static/build/gulpfile.js"
  },
  "keywords": [
    "blog"
  ],
  "author": "Zhongyang Ren",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-router": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "del": "^5.1.0",
    "eslint": "^5.16.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-imagemin": "^6.1.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "html-webpack-plugin": "^3.2.0",
    "klaw": "^3.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "mocker-api": "^1.8.1",
    "nodemon": "^1.19.1",
    "portfinder": "^1.0.23",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.1.0",
    "vue-eslint-parser": "^6.0.4",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
